# AWS Account Configuration
AWS_ACCOUNT_ID=123456789012
AWS_REGION=us-east-1

# Domain Configuration (Optional - for custom domain)
# DOMAIN_NAME=pixecom.com
# CERTIFICATE_ARN=arn:aws:acm:us-east-1:123456789012:certificate/xxxxx

# Origin Configuration
# Replace with your actual server IP or load balancer DNS
ORIGIN_DOMAIN=your-server-ip-or-lb.com
API_ORIGIN_DOMAIN=your-server-ip-or-lb.com

# Environment
ENVIRONMENT=production

# CloudFront Configuration
PRICE_CLASS=PriceClass_100
# Options: PriceClass_100 (US, Canada, Europe)
#          PriceClass_200 (adds Asia, Middle East, Africa)
#          PriceClass_All (all edge locations)

# WAF (Web Application Firewall) - Optional
ENABLE_WAF=true

# Logging
ENABLE_ACCESS_LOGS=true
